{
  "kind": "collectionType",
  "collectionName": "cats",
  "info": {
    "singularName": "cat",
    "pluralName": "cats",
    "displayName": "Cats",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "adopted": {
      "type": "boolean",
      "default": false
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "socialSkills": {
      "type": "component",
      "repeatable": false,
      "component": "cat-field.social-cat"
    },
    "gender": {
      "type": "enumeration",
      "enum": ["Mâle", "Femelle"],
      "required": true,
      "default": "Mâle"
    },
    "age": {
      "type": "string",
      "default": "Inconnu"
    },
    "robe": {
      "type": "string",
      "default": "Inconnu"
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "sicknesses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sickness.sickness"
    }
  }
}
